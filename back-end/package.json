{
	"name": "back-end",
	"version": "1.0.0",
	"main": "server.js",
	"scripts": {
		"test": "jest",
		"test:report": "jest --reporters=default --reporters=jest-html-reporter",
		"test:cases": "SET CASE_LOG=1&& SET SHOW_IO=0&& jest --runInBand --reporters=default --reporters=jest-html-reporter",
		"test:logs": "SET SHOW_IO=1&& jest --runInBand",
		"test:logs:save": "SET SHOW_IO=1&& jest --runInBand > test-report/terminal.log 2>&1",
		"logs:csv": "node scripts/parse_jest_logs_to_csv.js test-report/terminal.log test-report/terminal.csv",
		"start": "nodemon --exec ./node_modules/.bin/babel-node src/server.js",
		"run": "node --experimental-specifier-resolution=node src/server.js"
	},
	"author": "MINH",
	"license": "ISC",
